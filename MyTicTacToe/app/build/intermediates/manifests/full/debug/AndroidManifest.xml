    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="25" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.1" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.administrator.mytictactoe.MainActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.administrator.mytictactoe.Real_GamePlay"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.administrator.mytictactoe.HelpActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.administrator.mytictactoe.AboutActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.administrator.mytictactoe.CootieActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.administrator.mytictactoe.Cootie_GamePlay"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.administrator.mytictactoe.TicTacToeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.administrator.mytictactoe.TicTacToeHelpActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.administrator.mytictactoe.CootieHelpActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>